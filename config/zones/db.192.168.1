; MODIFY THIS FILE TO MATCH YOUR DOMAIN AND EMAIL ADDRESS
$TTL	604800
@	IN	SOA	example.com. youremail.gmail.com. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

; Name servers
	IN	NS	ns1.example.com.

; PTR records
; The first column should match the last octet of the IP address
; for which the reverse lookup applies.
; e.g., webserver.example.com can be found at 192.168.1.3	
2	IN	PTR	ns1.example.com.
3	IN	PTR	webserver.example.com.
4	IN	PTR	www.example.com.
